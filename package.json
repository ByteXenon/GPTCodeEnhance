{
  "name": "gpt-code-enhance",
  "repository": {
    "type": "git",
    "url": "https://github.com/ByteXenon/GPTCodeEnhance"
  },
  "icon": "resources/icon.png",
  "displayName": "GPT Code Enhance",
  "description": "A free VSCode extension to boost your productivity",
  "version": "1.0.2",
  "publisher": "ByteXenon",
  "main": "./extension.js",
  "engines": {
    "vscode": ">=1.60.0"
  },
  "categories": [
    "Formatters",
    "Programming Languages"
  ],
  "keywords": [
    "code refactoring",
    "code optimization",
    "code improvement",
    "code documentation",
    "source code",
    "programming",
    "coding",
    "software development",
    "developer tools",
    "code quality",
    "code review",
    "code analysis",
    "coding standards",
    "best practices",
    "API integration",
    "chatbot",
    "GPT",
    "OpenAI",
    "ChatGPT"
  ],
  "activationEvents": [
  ],
  "dependencies": {
    "axios": "^1.4.0"
  },
  "contributes": {
    "extensionResources": [
      {
        "title": "Documentation",
        "url": "https://github.com/ByteXenon/GPTCodeEnhance/wiki"
      },
      {
        "title": "Issue Tracker",
        "url": "https://github.com/ByteXenon/GPTCodeEnhance/issues"
      },
      {
        "title": "Changelog",
        "url": "https://github.com/ByteXenon/GPTCodeEnhance/blob/main/CHANGELOG.md"
      }
    ],
    "commands": [
      {
        "command": "extension.refactorCode",
        "title": "[GPT Enhance] Refactor Code"
      },
      {
        "command": "extension.improveCode",
        "title": "[GPT Enhance] Improve Code"
      },
      {
        "command": "extension.modularizeCode",
        "title": "[GPT Enhance] Modularize Code"
      },
      {
        "command": "extension.documentCode",
        "title": "[GPT Enhance] Document Code"
      },
      {
        "command": "extension.improveDesignPatterns",
        "title": "[GPT Enhance] Improve Design Patterns"
      },
      {
        "command": "extension.enhanceCodeFromComments",
        "title": "[GPT Enhance] Enhance with tag"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.refactorCode",
          "group": "3_modification",
          "when": "editorHasSelection"
        },
        {
          "command": "extension.improveCode",
          "group": "3_modification",
          "when": "editorHasSelection"
        },
        {
          "command": "extension.modularizeCode",
          "group": "3_modification",
          "when": "editorHasSelection"
        },
        {
          "command": "extension.documentCode",
          "group": "3_modification",
          "when": "editorHasSelection"
        },
        {
          "command": "extension.enhanceCodeFromComments",
          "group": "3_modification",
          "when": "editorHasSelection"
        },
        {
          "command": "extension.improveDesignPatterns",
          "group": "3_modification",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/ByteXenon/GPTCodeEnhance/issues",
    "email": "danielgtdavis@proton.me"
  }
}